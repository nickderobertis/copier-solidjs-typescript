{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "dev": "concurrently npm:dev:*",
    "dev-prod": "pnpm build && pnpm serve",
    "dev:server": "vite",
    "dev:css": "pnpm build:css --watch",
    "build": "pnpm build:css && pnpm build:vite",
    "build:css": "tailwindcss -i ./src/index.css -o ./src/styles.css",
